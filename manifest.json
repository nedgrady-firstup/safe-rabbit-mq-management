{
  "manifest_version": 3,
  "name": "Safe Rabbit MQ Management",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "icon": "safe-rabbit.png",
    "default_popup": "hello.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "css": ["remove-delete-queue.css"],
      "js": ["remove-purge-queue.js", "remove-move-messages.js", "suggest-move-to-production.js", "hermes_mailer_dlq.js"],
      "matches": ["https://*.mq.eu-west-1.amazonaws.com/", "https://*.mq.us-east-1.amazonaws.com/"]
    }
  ]
}